{
    "name": "Conversor números",
    "version": "1.1",
    "description": "Transforma números em dias trabalhados",
    "browser_action":{
        "default_popup": "index.html",
        "default_title": "Conversor para assalariados"
    },

    "content_scripts": [
        {
            "matches": ["https://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],

    "manifest_version": 2
}